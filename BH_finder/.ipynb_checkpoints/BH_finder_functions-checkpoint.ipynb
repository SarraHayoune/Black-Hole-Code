    "def loader(Path):\n",
    "    '''returns snap and halo'''\n",
    "    s=pynbody.load(Path)\n",
    "    s.physical_units()\n",
    "    h=s.halos()\n",
    "    \n",
    "    return (s,h)"
   ]
